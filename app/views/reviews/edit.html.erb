<div class="edit-review-body">
  <h1>投稿編集</h1>
    <div class="col-md-6 col-md-offset-3">
      <%= form_with model: @review,  local: true, url: review_path(@review) do |f| %>
          <div class="form-group">
            <%= f.text_area :content, :required => true, class: "form-control"%>
          </div>
          <div class="form-group">
            <%= f.label :アンケート画像, class: "label-review-new" %>
            <%= f.attachment_field :image, class: "enquete"%>
          </div>
          <%= f.submit "編集する", class:  "btn btn-primary" %>
      <% end %>
    </div>
</div>