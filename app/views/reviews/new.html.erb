<div class="container-fluid jumbotron-position">
  <h1>新規投稿</h1>
    <div class="col-md-6 col-md-offset-3">
      <%= form_with model: @review, url: reviews_path(@review) do |f| %>
        <div class="form-group">
          <%= f.text_area :content, :required => true, class: "form-control"%>
        </div>
        <div class="form-group">
          <%= f.label :アンケート画像, class: "label-review-new" %>
          <%= f.attachment_field :image, class: "enquete"%>
        </div>
        <%= f.submit "投稿する", class:  "btn btn-primary" %>
      <% end %>
    </div>
</div>