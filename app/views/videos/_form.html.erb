<%= form_with(model: @video, local: true) do |f| %>
  <%= render 'share/error_messages' , object: @video %>

  <%= f.label :title, class: "label-signup" %>
  <%= f.text_field :title, class: "form-control" %>

  <%= f.label :youtube_url, class: "label-signup" %>
  <%= f.text_field :youtube_url, class: "form-control" %>

  <%= f.submit yield(:button_text), class: "btn btn-primary btn-block btn-signup" %>
<% end %>